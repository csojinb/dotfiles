[user]
	name = Clara Bennett
	email = csojinb@gmail.com
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/Clara/.gitignore_global
	editor = nano
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	unstage = reset HEAD --
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[push]
	default = simple
[rebase]
	autosquash = true
[merge]
	tool = kdiff3
